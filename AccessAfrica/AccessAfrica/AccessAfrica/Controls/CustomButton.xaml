<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AccessAfrica.Controls.CustomButton">
    <StackLayout Padding="30,10,30,15" BackgroundColor="#FFFFFF">
        <StackLayout VerticalOptions="EndAndExpand">
            <Button x:Name="customButton"  Text="Proceed" Clicked="Button_Clicked" Style="{DynamicResource ButtonStyle}" />
        </StackLayout>
    </StackLayout>
</ContentView>